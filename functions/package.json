{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "eslint .",
    "serve": "development=true firebase emulators:start --only functions",
    "shell": "development=true firebase functions:shell",
    "start": "npm run shell",
    "runtime": "firebase functions:config:get > .runtimeconfig.json",
    "deploy": "firebase deploy --only functions && say done || say fail",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "16"
  },
  "main": "index.js",
  "dependencies": {
    "aws-sdk": "^2.1148.0",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "csso": "^5.0.3",
    "express": "^4.18.1",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.18.0",
    "graphql": "^16.5.0",
    "isomorphic-fetch": "^3.0.0",
    "juice": "^8.0.0",
    "promise-parallel-throttle": "^3.3.0",
    "promise-retry": "^2.0.1",
    "pug": "^3.0.2",
    "urql": "^2.2.1",
    "uuid": "^8.3.2",
    "web3": "^1.7.3"
  },
  "devDependencies": {
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^0.2.0"
  },
  "private": true
}
